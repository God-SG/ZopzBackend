    tailwind.config = {
        theme: {
            extend: {
                colors: {
                    primary: '#7289da',
                    secondary: '#5865f2',
                    dark: {
                        DEFAULT: '#1a1a1a',
                        deeper: '#141414',
                        card: '#202020'
                    }
                }
            }
        }
